{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "wispr",
  "version": "0.1.0",
  "identifier": "com.wisp-r.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "wispr",
        "width": 800,
        "height": 800,
        "minWidth": 500,
        "minHeight": 800,
        "focus": false,
        "acceptFirstMouse": true,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
